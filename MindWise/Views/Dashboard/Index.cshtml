@model List<DAL.Entities.CardClass>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/Scripts/Dashboard/Dashboard.js"></script>

<div class="row">
    <div class="col-xlg-7 col-lg-7">
    </div>
    <div class="col-xlg-3 col-lg-5 col-visitors">
        <div class="row" style="display:none">
            <div class="col-md-12">
                <div class="panel no-bd bd-3 panel-stat">
                    <div class="panel-body bg-red p-15">
                        <div class="row m-b-10">
                            <div class="col-xs-3 big-icon">
                                <i class="icon-equalizer"></i>
                            </div>
                            <div class="col-xs-9">
                                <small class="stat-title">ORDERS THIS MONTH</small>
                                <div class="live-tile" data-mode="carousel" data-direction="vertical" data-delay="3500" data-height="56">
                                    <div>
                                        <h1 class="f-40 m-0 w-300">148</h1>
                                    </div>
                                    <div>
                                        <h1 class="f-40 m-0 w-300">+28%</h1>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-6">
                                <small class="stat-title">Last month</small>
                                <div class="live-tile" data-mode="carousel" data-direction="vertical" data-delay="3500" data-height="23">
                                    <div>
                                        <h3 class="f-20 m-0 w-300">126</h3>
                                    </div>
                                    <div>
                                        <h3 class="f-20 m-0 w-300">$12,545</h3>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-6">
                                <small class="stat-title">Last week</small>
                                <div class="live-tile f-right" data-mode="carousel" data-direction="vertical" data-delay="3500" data-height="23">
                                    <div>
                                        <h3 class="t-right f-20 m-0 w-500">43</h3>
                                    </div>
                                    <div>
                                        <h3 class="t-right f-20 m-0 w-500">$4,237</h3>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-xlg-12 col-financial-stocks">
        <div class="panel">
            <div class="panel-header panel-controls">
                <h3><i class="fa fa-table"></i>Card Class</h3>
            </div>
            <div class="panel-content widget-full widget-stock stock1">
                <div class="tabs tabs-linetriangle">
                    <ul class="nav nav-tabs nav-4" role="tablist">
                        @foreach (var item in @Model)
                        {
                            <li class="CardDetails" role="tab" data-toggle="tab">
                                @*<li class="nav-item lines-3" role="tab" data-toggle="tab">*@
                                <a href="#microsoft-tab" id="microsoft" data-toggle="tab">
                                    <span class="title">@item.ClassName</span>
                                    <span class="c-gray"><strong>@item.CardCount</strong></span>
                                </a>
                            </li>

                        }
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xlg-2 col-small-stats">
    </div>
    <div class="col-xlg-6">
    </div>
</div>
<div class="row">
    <div class="col-lg-12 portlets ui-sortable">
        <div class="panel">
            <div class="panel-header panel-controls ui-sortable-handle">
                <h3><i class="fa fa-table"></i> CVV LDG</h3>
                <div class="control-btn"><a href="#" class="panel-reload hidden"><i class="icon-reload"></i></a><a class="hidden" id="dropdownMenu1" data-toggle="dropdown"><i class="icon-settings"></i></a><ul class="dropdown-menu pull-right" role="menu" aria-labelledby="dropdownMenu1"><li><a href="#">Action</a></li><li><a href="#">Another action</a></li><li><a href="#">Something else here</a></li></ul><a href="#" class="panel-popout hidden tt" title="Pop Out/In"><i class="icons-office-58"></i></a><a href="#" class="panel-maximize hidden"><i class="icon-size-fullscreen"></i></a><a href="#" class="panel-toggle"><i class="fa fa-angle-down"></i></a><a href="#" class="panel-close"><i class="icon-trash"></i></a></div>
            </div>
            <div class="row">
                <div class="col-sm-6">
                </div>
                <div class="col-sm-6">
                    <div id="table2_filter" class="dataTables_filter">
                        <div class="row">
                            <div class="col-sm-4">
                                @*<input type="search" class="form-control input-sm" placeholder="" aria-controls="table2">*@
                                <input type="text" id="txtSearchbyUsername" class="form-control" placeholder="Search by Username">
                            </div>
                            <div class="col-sm-4">
                                <input type="text" id="txtSearchbyBin" class="form-control" placeholder="Search by Bin">
                            </div>
                            <div class="col-sm-4">
                                <button type="button" id="btnSearchCVVLDG" class="btn btn-md btn-default">Search</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel-content pagination2 table-responsive">
                @Html.Partial("~/Views/PartialView/CVVLDG.cshtml")
            </div>
        </div>
    </div>
</div>
